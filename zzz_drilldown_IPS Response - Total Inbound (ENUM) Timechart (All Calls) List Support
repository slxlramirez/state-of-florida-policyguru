        <search base="cdr">
          <query>| eval _time=strptime(flowtime_ts_utc,"%Y-%m-%d %H:%M:%S")
| timechart fixedrange=false span=1m dc(sourcenumber) as sources dc(destinationnumber) as destinations</query>
        </search>
        <option name="charting.chart">line</option>
