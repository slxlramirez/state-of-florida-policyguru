    <panel>
      <table>
        <title>Top 10 Source Numbers</title>
        <search base="cdr">
          <query>| stats count by sourcenumber
| sort -count
| head 10</query>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
