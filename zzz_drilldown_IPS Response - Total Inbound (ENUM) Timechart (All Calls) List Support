          <query>| dbxquery 
connection="ngp_connection" 
timeout=1800 query="
SELECT to_char(to_timestamp(cast($date_value$ as bigint)),'YYYY-MM-DD hh24:mi') as c
" shortnames=t
| table *</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
      </single>
    </panel>
